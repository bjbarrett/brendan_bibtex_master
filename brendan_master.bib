@article{Eze2020,
  title        = {{Feasible crop insurance indexes for drought risk management in Northern Ethiopia}},
  author       = {Eze, Emmanuel and Girma, Atkilt and Zenebe, Amanuel A. and Zenebe, Gebreyohannes},
  year         = 2020,
  journal      = {International Journal of Disaster Risk Reduction},
  publisher    = {Elsevier Ltd},
  volume       = 47,
  number       = {February},
  pages        = 101544,
  doi          = {10.1016/j.ijdrr.2020.101544},
  issn         = 22124209,
  url          = {https://doi.org/10.1016/j.ijdrr.2020.101544},
  abstract     = {Rainfed agriculture output is constantly at risk of being influenced by rainfall deficits and drought events. The northern part of Ethiopia, has witnessed historical meteorological and agricultural drought events, which have highly impacted on smallholders' crop yield. With the frequency and severity of drought events expected to increase in the future due to predicted climatic changes, the need for risk-reduction mechanisms is overt. Crop insurance is an innovative risk management mechanism during extreme weather events like droughts, with index-based insurance designs being more popular and feasible in developing countries where traditional crop insurance is unattainable. This study therefore utilized crop yield data from survey of 34 randomly selected farmers' plots to develop payout thresholds related to reported crop losses during historical drought years to develop a crop insurance index to be triggered by drought. The kiremt (summer) DevNDVI shows the closest proxy to crop losses recorded in the area with a lower threshold of 1.31Qt/ha. The indexes designed by this study performed very well when payout conditions were evaluated in the light of the recent drought years in the area. We therefore advocate the consideration of area-specific crop insurance index for implementation in Ethiopia at large, being more compensatory than the current policies based on weather derivatives.},
  file         = {:C$\backslash$:/Users/Kate/Google Drive/cimmyt{\_}cropcuts/papers/1-s2.0-S2212420919310143-main.pdf:pdf},
  keywords     = {Climate change,Climatic risk,Cluster analysis,Crop insurance,Drought,Tigray}
}
