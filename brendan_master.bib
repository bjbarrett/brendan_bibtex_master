@article{Mwakatobe2014,
  title        = {{The impact of crop raiding by wild animals in communities surrounding the Serengeti National Park, Tanzania}},
  author       = {Mwakatobe, Angela and Nyahongo, Julius and Ntalwila, Janemary and Roskaft, Eivin},
