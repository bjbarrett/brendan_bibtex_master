@article{haun_majority-biased_2012,
  title        = {Majority-{Biased} {Transmission} in {Chimpanzees} and {Human} {Children}, but {Not} {Orangutans}},
  author       = {Haun, Daniel B.M. and Rekers, Yvonne and Tomasello, Michael},
  year         = 2012,
  month        = apr,
  journal      = {Current Biology},
  volume       = 22,
  number       = 8,
  pages        = {727--731},
  doi          = {10.1016/j.cub.2012.03.006},
  issn         = {0960-9822},
  url          = {https://www.sciencedirect.com/science/article/pii/S0960982212002631},
  urldate      = {2013-09-18},
  noopurl      = {http://www.sciencedirect.com/science/article/pii/S0960982212002631},
  abstract     = {Summary Cultural transmission is a key component of human evolution. Two of humans' closest living relatives, chimpanzees and orangutans, have also been argued to transmit behavioral traditions across generations culturally [1–3], but how much the process might resemble the human process is still in large part unknown. One key phenomenon of human cultural transmission is majority-biased transmission: the increased likelihood for learners to end up not with the most frequent behavior but rather with the behavior demonstrated by most individuals. Here we show that chimpanzees and human children as young as 2 years of age, but not orangutans, are more likely to copy an action performed by three individuals, once each, than an action performed by one individual three times. The tendency to acquire the behaviors of the majority has been posited as key to the transmission of relatively safe, reliable, and productive behavioral strategies [4–7] but has not previously been demonstrated in primates.},
  file         = {ScienceDirect Full Text PDF:/Users/BJB/Library/Application Support/Zotero/Profiles/kcgzlx51.default/zotero/storage/6ZAVZ3JC/Haun et al. - 2012 - Majority-Biased Transmission in Chimpanzees and Hu.pdf:application/pdf;ScienceDirect Snapshot:/Users/BJB/Library/Application Support/Zotero/Profiles/kcgzlx51.default/zotero/storage/65W7725S/S0960982212002631.html:text/html}
}
