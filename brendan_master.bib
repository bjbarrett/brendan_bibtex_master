@article{dindo_-group_2009,
  title        = {In-{Group} {Conformity} {Sustains} {Different} {Foraging} {Traditions} in {Capuchin} {Monkeys} ({Cebus} apella)},
  author       = {Dindo, Marietta and Whiten, Andrew and {de Waal}, Frans B. M.},
  year         = 2009,
  month        = nov,
  journal      = {PLoS ONE},
  publisher    = {Public Library of Science},
  volume       = 4,
  number       = 11,
  pages        = {e7858},
  doi          = {10.1371/journal.pone.0007858},
  url          = {https://doi.org/10.1371/journal.pone.0007858},
  urldate      = {2013-09-18},
  noopurl      = {http://dx.doi.org/10.1371/journal.pone.0007858},
  abstract     = {
    Background Decades of research have revealed rich cultural repertoires encompassing multiple traditions in wild great apes, a picture crucially complemented by experimental simulations with captive apes. Studies with wild capuchin monkeys, the most encephalized simian species, have indicated a New World convergence on these cultural phenomena, involving multiple traditions and tool use. However, experimental studies to date are in conflict with such findings in concluding that capuchins, like other monkeys, show minimal capacities for social learning.

    Methodology/Principal Findings Here we report a new experimental approach in which the alpha male of each of two groups of capuchins was trained to open an artificial foraging device in a quite different way, using either a slide or lift action, then reunited with his group. In each group a majority of monkeys, 8 of 11 and 13 of 14, subsequently mastered the task. Seventeen of the successful 21 monkeys discovered the alternative action to that seeded in the group, performing it a median of 4 times. Nevertheless, all 21 primarily adopted the technique seeded by their group's alpha male. Median proportions of slide versus lift were 0.96 for the group seeded with slide versus 0. 01 for the group seeded with lift.

    Conclusions/Significance These results suggest a striking effect of social conformity in learned behavioral techniques, consistent with field reports of capuchin traditions and convergent on the only other species in which such cultural phenomena have been reported, chimpanzees and humans.
  },
  file         = {PLoS Full Text PDF:/Users/BJB/Library/Application Support/Zotero/Profiles/kcgzlx51.default/zotero/storage/9GC3R7KI/Dindo et al. - 2009 - In-Group Conformity Sustains Different Foraging Tr.pdf:application/pdf;PLoS Snapshot:/Users/BJB/Library/Application Support/Zotero/Profiles/kcgzlx51.default/zotero/storage/G5S9REGM/infodoi10.1371journal.pone.html:text/html}
}
@article{dindo_social_2008,
  title        = {Social diffusion of novel foraging methods in brown capuchin monkeys ({Cebus} apella)},
  author       = {Dindo, Marietta and Thierry, Bernard and Whiten, Andrew},
  year         = 2008,
  month        = jan,
  journal      = {Proceedings of the Royal Society B: Biological Sciences},
  publisher    = {The Royal Society},
  volume       = 275,
  number       = 1631,
  pages        = {187--193},
  doi          = {10.1098/rspb.2007.1318},
  issn         = {0962-8452, 1471-2954},
  url          = {http://rspb.royalsocietypublishing.org/content/275/1631/187},
  urldate      = {2013-09-19},
  noopurl      = {http://rspb.royalsocietypublishing.org/content/275/1631/187},
  abstract     = {It has been reported that wild capuchin monkeys exhibit several group-specific behavioural traditions. By contrast, experiments have found little evidence for the social learning assumed necessary to support such traditions. The present study used a diffusion chain paradigm to investigate whether a novel foraging task could be observationally learned by capuchins (Cebus apella) and then transmitted along a chain of individuals. We used a two-action paradigm to control for independent learning. Either of two methods (lift or slide) could be used to open the door of a foraging apparatus to retrieve food. Two chains were tested (N1=4; N2=5), each beginning with an experimenter-trained model who demonstrated to a partner its group-specific method for opening the foraging apparatus. After the demonstration, if the observer was able to open the apparatus 20 times by either method, then it became the demonstrator for a new subject, thus simulating the spread of a foraging tradition among ‘generations’ of group members. Each method was transmitted along these respective chains with high fidelity, echoing similar results presently available only for chimpanzees and children. These results provide the first clear evidence for faithful diffusion of alternative foraging methods in monkeys, consistent with claims for capuchin traditions in the wild.},
  language     = {en},
  pmid         = 17971322,
  keywords     = {Cebus apella, Culture, diffusion chains, Primates, social learning, tradition},
  file         = {Full Text PDF:/Users/BJB/Library/Application Support/Zotero/Profiles/kcgzlx51.default/zotero/storage/CQ9T36II/Dindo et al. - 2008 - Social diffusion of novel foraging methods in brow.pdf:application/pdf;Snapshot:/Users/BJB/Library/Application Support/Zotero/Profiles/kcgzlx51.default/zotero/storage/6WPTIZC8/187.html:text/html},
  eprint       = {http://rspb.royalsocietypublishing.org/content/275/1631/187.full.pdf}
}
