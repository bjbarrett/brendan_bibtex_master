  abstract     = {A goal of many research programmes in biology is to extract meaningful insights from large, complex datasets. Researchers in ecology, evolution and behavior (EEB) often grapple with long-term, observational datasets from which they construct models to test causal hypotheses about biological processes. Similarly, epidemiologists analyse large, complex observational datasets to understand the distribution and determinants of human health. A key difference in the analytical workflows for these two distinct areas of biology is the delineation of data analysis tasks and explicit use of causal directed acyclic graphs (DAGs), widely adopted by epidemiologists. Here, we review the most recent causal inference literature and describe an analytical workflow that has direct applications for EEB. We start this commentary by defining four distinct analytical tasks (description, prediction, association, causal inference). The remainder of the text is dedicated to causal inference, specifically focusing on the use of DAGs to inform the modelling strategy. Given the increasing interest in causal inference and misperceptions regarding this task, we seek to facilitate an exchange of ideas between disciplinary silos and provide an analytical framework that is particularly relevant for making causal inference from observational data.}
}
@article{leadbeater_social_2007,
  title        = {Social {Learning} in {Insects} — {From} {Miniature} {Brains} to {Consensus} {Building}},
  author       = {Leadbeater, Ellouise and Chittka, Lars},
  year         = 2007,
  month        = aug,
  journal      = {Current Biology},
  volume       = 17,
  number       = 16,
  pages        = {R703--R713},
  doi          = {10.1016/j.cub.2007.06.012},
  issn         = {0960-9822},
  urldate      = {2013-09-23},
  noopurl      = {http://www.sciencedirect.com/science/article/pii/S096098220701514X},
  abstract     = {Communication and learning from each other are part of the success of insect societies. Here, we review a spectrum of social information usage in insects — from inadvertently provided cues to signals shaped by selection specifically for information transfer. We pinpoint the sensory modalities involved and, in some cases, quantify the adaptive benefits. Well substantiated cases of social learning among the insects include learning about predation threat and floral rewards, the transfer of route information using a symbolic ‘language’ (the honeybee dance) and the rapid spread of chemosensory preferences through honeybee colonies via classical conditioning procedures. More controversial examples include the acquisition of motor memories by observation, teaching in ants and behavioural traditions in honeybees. In many cases, simple mechanistic explanations can de identified for such complex behaviour patterns.},
  file         = {ScienceDirect Full Text PDF:/Users/BJB/Library/Application Support/Zotero/Profiles/kcgzlx51.default/zotero/storage/ETT36V68/Leadbeater and Chittka - 2007 - Social Learning in Insects — From Miniature Brains.pdf:application/pdf;ScienceDirect Snapshot:/Users/BJB/Library/Application Support/Zotero/Profiles/kcgzlx51.default/zotero/storage/FG7P8JW7/S096098220701514X.html:text/html}
}
