  keywords     = {Behavioural Sciences, Cebus apella, Human Physiology, Learning, Primates, Social cognition, tool use, Zoology},
  file         = {Snapshot:/Users/BJB/Library/Application Support/Zotero/Profiles/kcgzlx51.default/zotero/storage/6TQWV2F5/s10071-004-0245-8.html:text/html},
  date-added   = {2018-02-22 12:10:07 +0000},
